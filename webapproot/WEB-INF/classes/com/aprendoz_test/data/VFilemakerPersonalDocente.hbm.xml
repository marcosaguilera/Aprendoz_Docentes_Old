<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_test.data.VFilemakerPersonalDocente" table="v_filemaker_personal_docente" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <composite-id name="id" class="com.aprendoz_test.data.VFilemakerPersonalDocenteId">
            <key-property name="idPersona" type="integer">
                <column name="Id_Persona"/>
            </key-property>
            <key-property name="apellido1" type="string">
                <column name="Apellido1" length="45"/>
            </key-property>
            <key-property name="apellido2" type="string">
                <column name="Apellido2" length="45"/>
            </key-property>
            <key-property name="nombre1" type="string">
                <column name="Nombre1" length="45"/>
            </key-property>
            <key-property name="nombre2" type="string">
                <column name="Nombre2" length="45"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>